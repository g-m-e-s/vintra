                         "formidable":  "^3.5.2"
                     },
    "devDependencies":  {
                            "@types/cors":  "^2.8.17",
                            "@types/express":  "^4.17.21",
                            "@types/node":  "^18.19.86",
                            "cross-env":  "^7.0.3",
                            "typescript":  "^5.8.3"
                        },
    "engines":  {
                    "node":  "18"
                }
}
